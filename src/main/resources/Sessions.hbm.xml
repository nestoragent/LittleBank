<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.littlebank.logic.BankAccount" table="sys.bankAccount">
        <id column="session_id" name="id" type="java.lang.Integer">
            <generator class="increment"/>
        </id>
        <property column="timeStart" name="timeStart" type="java.lang.String"/>
        <property column="timeEnd" name="timeEnd" type="java.lang.String"/>

    </class>
</hibernate-mapping>