<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.littlebank.logic.BankAccount" table="sys.bankAccount">
        <id column="account_id" name="account_id" type="java.lang.Long">
            <generator class="increment"/>
        </id>
        <property column="accountNumber" name="accountNumber" type="java.lang.Long"/>
        <property column="IBAN" name="IBAN" type="java.lang.String"/>
        <property column="bankName" name="bankName" type="java.lang.String"/>
        <property column="bic" name="bic" type="java.lang.Long"/>
    </class>
</hibernate-mapping>